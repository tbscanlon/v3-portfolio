---
import Box from "./Box.astro";
import interests from "../lib/interests.json";

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<Box
  type="playing"
  title={interests.playing.title}
  description={interests.playing.developer}
  color="purple"
  href={interests.playing.url}
  class={className}
/>
