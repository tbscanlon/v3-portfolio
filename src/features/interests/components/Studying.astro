---
import Box from "./Box.astro";
import interests from "../lib/interests.json";

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<Box
  type="learning"
  title={interests.studying.title}
  description={interests.studying.author}
  color="blue"
  href={interests.studying.url}
  class={className}
/>
