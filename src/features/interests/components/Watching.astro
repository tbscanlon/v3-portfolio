---
import Box from "./Box.astro";
import interests from "../lib/interests.json";

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<Box
  type="watching"
  title={interests.watching.title}
  description={interests.watching.genres}
  color="green"
  href={interests.watching.url}
  class={className}
/>
