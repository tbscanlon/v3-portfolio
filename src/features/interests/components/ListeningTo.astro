---
import Box from "./Box.astro";
import interests from "../lib/interests.json";

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<Box
  type="listening to"
  title={interests.listening.title}
  description={interests.listening.artist}
  color="orange"
  href={interests.listening.url}
  class={className}
/>
