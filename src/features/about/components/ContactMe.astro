---
import { getEntry } from "astro:content";
import AboutSection from "./AboutSection.astro";

const contact = await getEntry("about", "contact");
const { Content } = await contact.render();
---

<AboutSection
  title={contact.data.title}
  color={contact.data.color}
  id="contact"
>
  <Content slot="content" />
</AboutSection>
